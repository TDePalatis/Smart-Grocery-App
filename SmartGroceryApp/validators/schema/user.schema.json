{
    "schema": "https://json-schema.org/draft/2020-12/schema",
    "title": "User",
    "type": "object",
    "properties": {
      "email": { "type": "string", "format": "email" },
      "password": { "type": "string", "minLength": 6 },
      "created_at": { "type": "string", "format": "date-time" }
    },
    "required": ["email", "password"],
    "additionalProperties": false
  }
  